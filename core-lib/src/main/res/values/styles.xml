    <style name="AppTheme.NoActionBar" parent="SwipeTheme">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary_d</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_d</item>
        <item name="colorAccent">@color/colorAccent_d</item>
    </style>


    <!-- animation 样式 -->
    <style name="activityAnimation" parent="@android:style/Animation">
        <item name="android:activityOpenEnterAnimation">@anim/slide_right_in</item>
        <item name="android:activityOpenExitAnimation">@anim/slide_left_out</item>
        <item name="android:activityCloseEnterAnimation">@anim/slide_left_in</item>
        <item name="android:activityCloseExitAnimation">@anim/slide_right_out</item>
    </style>


    <style name="SwipeTheme" parent="AppTheme">
        <!-- Customize your theme here. -->
        <!--//背景透明，不设滑动关闭时背景就是黑的。-->
        <item name="android:windowIsTranslucent">true</item>
        <!--//Activity右滑进出的动画，觉得这个不好看随便换成自己的-->
        <item name="colorPrimary">@color/colorPrimary_d</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_d</item>
        <item name="colorAccent">@color/colorAccent_d</item>
    </style>

    <style name="MainActivityTheme" parent="AppTheme">
        <!-- Customize your theme here. -->
        <!--//背景透明，不设滑动关闭时背景就是黑的。-->
        <item name="android:windowIsTranslucent">false</item>
        <!--//Activity右滑进出的动画，觉得这个不好看随便换成自己的-->
        <item name="android:windowAnimationStyle">@style/activityAnimation</item>
        <item name="colorPrimary">@color/colorPrimary_d</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark_d</item>
        <item name="colorAccent">@color/colorAccent_d</item>
    </style>


