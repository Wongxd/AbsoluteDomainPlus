<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--切换主题用的自定义颜色  开始-->
    <attr name="content_board_background_color" format="color" />
    <attr name="board_background_color" format="color" />
    <attr name="primary_color" format="color" />
    <attr name="accent_color" format="color" />
    <!--切换主题用的自定义颜色  结束-->

    <attr name="selected_color" format="color" />


    <declare-styleable name="DSListView">
        <attr name="picker_box_view" format="reference" />

        <attr name="data_array" format="integer" />
        <attr name="selected_index" format="integer" />

        <attr name="selector_background" format="reference|color" />

        <attr name="scale_animations" format="boolean" />
        <attr name="scale_animations_factor" format="float" />
        <attr name="scale_animations_pivot_center" format="boolean" />

        <attr name="cell_font_size" format="dimension" />
    </declare-styleable>

</resources>